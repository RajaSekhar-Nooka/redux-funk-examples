{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "no-console": "off",
    "no-fallthrough": "off",
    "no-case-declarations": "off"
  },
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint"
}
